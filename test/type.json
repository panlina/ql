{
	"post": {
		"id": ":number",
		"title": ":string",
		"body": ":string",
		"userId": ":number",
		"user": "=users#(this.userId)",
		"comments": "=::comments|postId=this post.id"
	},
	"comment": {
		"id": ":number",
		"name": ":string",
		"email": ":string",
		"body": ":string",
		"postId": ":number",
		"post": "=posts#(this.postId)"
	},
	"album": {
		"id": ":number",
		"title": ":string",
		"userId": ":number",
		"user": "=users#(this.userId)",
		"photos":"=::photos|albumId=this album.id"
	},
	"photo": {
		"id": ":number",
		"title": ":string",
		"url": ":string",
		"thumbnailUrl": ":string",
		"albumId": ":number",
		"album": "=albums#(this.albumId)"
	},
	"user": {
		"id": ":number",
		"name": ":string",
		"email": ":string",
		"posts": "=::posts|userId=this user.id",
		"albums": "=::albums|userId=this user.id",
		"todos": "=::todos|userId=this user.id"
	},
	"todo": {
		"id": ":number",
		"title": ":string",
		"completed": ":boolean",
		"userId": ":number",
		"user": "=users#(this.userId)"
	}
}