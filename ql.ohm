ql {
	Expression = ExpressionFilter
	ExpressionFilter =
		ExpressionFilter "|" ExpressionCompare	-- filter
		| ExpressionCompare
	ExpressionCompare =
		ExpressionCompare ("<="|"="|">="|"<"|"!="|">") ExpressionAtom	-- compare
		| ExpressionAtom
	ExpressionAtom =
		"(" Expression ")"	-- parentheses
		| ExpressionName
	ExpressionName = identifier
	identifier = alnum+
}